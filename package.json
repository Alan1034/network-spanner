    "@vitejs/plugin-vue": "^4.2.3",
    "@vitejs/plugin-vue-jsx": "^3.0.1",
    "chalk": "^2.4.2",
    "cross-env": "^7.0.3",
    "less": "^4.2.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^5.0.5",
    "tsx": "^4.0.0",
    "typescript": "^5.2.2",
    "vite-plugin-image-optimizer": "^1.1.6"
  },
